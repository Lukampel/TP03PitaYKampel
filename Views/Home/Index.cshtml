@{
    ViewData["Title"] = "Discos";
}


<main>
<div>
    
     @foreach (Disco disco in ViewBag.discos.Values) 
     {
        <article>
        <img src = "@disco.foto" alt = "Portada" width="300" height="auto">
       <p>@disco.nombreAlbum</p> 
       <a href = '@Url.Action("SelectDisco", new{id=@disco.ID})' alt = "Ir a datos del disco"> Ir los datos del disco</a>
        </article>
     } 
     
</div>

